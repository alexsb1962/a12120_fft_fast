function var=susb(arg)
%fn=fopen('r.txt','w');
%fprintf(fn,'%d\n\r',len);
%fprintf(fn,'%d\n\r',dev);
%fprintf(fn,'%d\n\r',chan);
%fclose(fn);
dos('debug\loader.exe '    );
